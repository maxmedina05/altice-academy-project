<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medma.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayoutVerticalActivity" />
        <activity android:name=".LinearLayoutHorizontalActivity" />
        <activity android:name=".RelativeLayoutActivity" />
        <activity android:name=".TableLayoutActivity" />
        <activity android:name=".FrameLayoutActivity" />
        <activity android:name=".ListLayoutActivity" />
        <activity android:name=".GridLayoutActivity" />
        <activity android:name=".ConstrainLayoutActivity" />
        <activity android:name=".UserAdapterActivity"></activity>
    </application>

</manifest>